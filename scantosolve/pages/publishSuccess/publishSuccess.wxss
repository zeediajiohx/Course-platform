/* pages/publishSuccess/publishSuccess.wxss */

.container{
  height: 600rpx;
  display:flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.container .title{
  font-size: 40rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.container .choose{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.container .choose .btn{
  padding: 20rpx;
  border: 1px solid #eb2709;
  width: 200rpx;
  text-align: center;
  color:white;
  border-radius: 60rpx;
  background-color: #f12e10;
  font-size: 30rpx;
}