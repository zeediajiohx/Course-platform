<view class="container">
    <view class="title"> 
      <icon type="success"></icon> 
      <text>发布成功</text>
      </view>
    <view class="choose">
      <view class="btn" style="background-color: white; color: tomato;"  bindtap="seeclass" open-type="switchTab">查看动态</view>
      <view class="btn" bindtap="continuePublish" open-type="switchTab">继续发布</view>
    </view>
</view>
