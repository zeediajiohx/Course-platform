    margin-right: 20rpx;
    padding: 10rpx 0;
  }
  
  .buttom-view .btn .publish{
    padding: 10rpx 20rpx;
    background-color: #f72222;
    color: white;
    border-radius: 10rpx;
  }
  
  .buttom-view .comment-area .hide{
    position: absolute;
    right: 0;
    top: -20rpx;
  }
